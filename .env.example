# API Configuration
REACT_APP_API_URL=http://localhost:5000/api

# Speech Recognition API (for voice input)
REACT_APP_SPEECH_API_KEY=your_speech_api_key_here

# Weather API Integration
REACT_APP_WEATHER_API_KEY=your_weather_api_key_here

# Google Maps API (for location services)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Firebase Configuration (if using Firebase for additional features)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id

# Analytics Configuration
REACT_APP_GOOGLE_ANALYTICS_ID=your_analytics_id_here

# Environment
NODE_ENV=development

# Application Configuration
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_DATE=2024-01-01
